<?xml version="1.0" ?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkListStore" id="list_store">
    <columns>
      <column type="GdkPixbuf"/>
      <column type="gchararray"/>
      <column type="gchararray"/>
      <column type="gpointer"/>
    </columns>
  </object>

  <object class="GtkListStore" id="include_store">
    <columns>
      <column type="gchararray"/>
    </columns>
  </object>

  <object class="GtkWindow" id="tip_window">
    <property name="type">GTK_WINDOW_POPUP</property>
    <property name="resizable">False</property>
    <property name="accept_focus">False</property>
    <property name="opacity">0.95</property>
    <child>
      <object class="GtkNotebook" id="notebook">
        <property name="show_tabs">False</property>
        <child>
          <object class="GtkTreeView" id="list_view">
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="list_label">
            <property name="label" translatable="yes">list</property>
          </object>
        </child>
        <child>
          <object class="GtkTreeView" id="include_view">
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="include_label">
            <property name="label" translatable="yes">include</property>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkSourceView" id="decl_view">
            <property name="editable">False</property>
            <property name="tab-width">4</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="decl_label">
            <property name="label" translatable="yes">decl</property>
          </object>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>